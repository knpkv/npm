{
  "private": true,
  "type": "module",
  "packageManager": "pnpm@10.17.1",
  "scripts": {
    "clean": "node scripts/clean.mjs",
    "codegen": "pnpm --recursive --parallel --filter \"./packages/**/*\" run codegen",
    "build": "tspc -b tsconfig.build.json && pnpm --recursive --filter \"./packages/**/*\" run build",
    "test": "vitest",
    "coverage": "vitest --coverage",
    "check": "pnpm --recursive --filter \"./packages/**/*\" run check",
    "lint": "eslint \"**/{src,test,examples,dtslint}/**/*.{ts,mjs}\"",
    "lint:fix": "pnpm lint --fix",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "changeset:version": "changeset version",
    "changeset:publish": "pnpm build && changeset publish",
    "prepare": "ts-patch install -s && husky",
    "effect-solutions": "effect-solutions"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.0",
    "@changesets/cli": "^2.29.7",
    "@effect/build-utils": "^0.8.9",
    "@effect/eslint-plugin": "^0.3.2",
    "@effect/language-service": "^0.45.1",
    "@effect/vitest": "^0.27.0",
    "@eslint/js": "^9.36.0",
    "@types/node": "^24.8.1",
    "@vitest/coverage-v8": "^4.0.13",
    "effect-solutions": "^0.4.13",
    "eslint": "^9.36.0",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import-x": "^4.16.1",
    "eslint-plugin-local-rules": "^3.0.2",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-sort-destructure-keys": "^2.0.0",
    "eslint-plugin-unused-imports": "^4.2.0",
    "glob": "^11.0.3",
    "husky": "^9.1.7",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "ts-patch": "^3.3.0",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.46.1",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^4.0.13"
  },
  "pnpm": {
    "patchedDependencies": {
      "@changesets/get-github-info@0.6.0": "patches/@changesets__get-github-info@0.6.0.patch",
      "@changesets/assemble-release-plan": "patches/@changesets__assemble-release-plan.patch"
    },
    "overrides": {
      "distilled-aws>fast-xml-parser": "^5.3.4",
      "vitest": "$vitest"
    }
  }
}
