{
  "name": "@knpkv/effect-ai-claude-code-cli",
  "version": "0.0.1",
  "type": "module",
  "description": "Effect-TS wrapper for Claude Code CLI with @effect/ai integration",
  "license": "MIT",
  "author": "knpkv",
  "repository": {
    "type": "git",
    "url": "https://github.com/knpkv/npm.git",
    "directory": "packages/effect-ai-claude-code-cli"
  },
  "bugs": {
    "url": "https://github.com/knpkv/npm/issues"
  },
  "homepage": "https://github.com/knpkv/npm/tree/main/packages/effect-ai-claude-code-cli#readme",
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": "./src/index.ts",
    "./*": "./src/*.ts",
    "./internal/*": null
  },
  "scripts": {
    "build": "tsc -b tsconfig.build.json && node scripts/build-package.js",
    "clean": "rimraf dist dist-test .tsbuildinfo",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:integration": "vitest run test/integration.test.ts",
    "lint": "eslint src examples",
    "check": "tsc --noEmit --project tsconfig.src.json && tsc --noEmit --project tsconfig.test.json",
    "check:examples": "tsc --noEmit --project tsconfig.examples.json",
    "check:all": "pnpm check && pnpm check:examples"
  },
  "peerDependencies": {
    "effect": "^3.19.3"
  },
  "dependencies": {
    "@effect/ai": "^0.32.0",
    "@effect/platform": "^0.93.0",
    "@effect/platform-node": "^0.87.0",
    "@effect/schema": "^0.75.0"
  },
  "devDependencies": {
    "@effect/vitest": "^0.15.0",
    "typescript": "~5.9.0",
    "vitest": "^2.0.0",
    "effect": "^3.19.3"
  },
  "keywords": [
    "effect",
    "effect-ts",
    "claude",
    "claude-code",
    "cli",
    "ai",
    "llm",
    "anthropic"
  ]
}
