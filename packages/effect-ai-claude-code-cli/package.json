{
  "name": "@knpkv/effect-ai-claude-code-cli",
  "version": "0.0.1",
  "type": "module",
  "description": "Effect-TS wrapper for Claude Code CLI with @effect/ai integration",
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "exports": {
    ".": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc -b tsconfig.build.json && pnpm run build:copy",
    "build:copy": "cp package.json dist/",
    "clean": "rimraf dist dist-test .tsbuildinfo",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src",
    "check": "tsc -b tsconfig.json --noEmit"
  },
  "peerDependencies": {
    "effect": "^3.19.3"
  },
  "dependencies": {
    "@effect/ai": "^0.32.0",
    "@effect/platform": "^0.93.0",
    "@effect/platform-node": "^0.87.0",
    "@effect/schema": "^0.75.0"
  },
  "devDependencies": {
    "@effect/vitest": "^0.15.0",
    "typescript": "~5.9.0",
    "vitest": "^2.0.0",
    "effect": "^3.19.3"
  },
  "keywords": [
    "effect",
    "effect-ts",
    "claude",
    "claude-code",
    "cli",
    "ai",
    "llm",
    "anthropic"
  ],
  "author": "",
  "license": "MIT"
}
