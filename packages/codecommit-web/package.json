{
  "name": "@knpkv/codecommit-web",
  "version": "0.3.0",
  "description": "Web server and frontend for CodeCommit PR browser",
  "type": "module",
  "main": "src/index.ts",
  "publishConfig": {
    "access": "public",
    "main": "dist/index.js",
    "types": "dist/dts/index.d.ts"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "bun run src/main.ts",
    "dev": "bun run --watch src/main.ts & vite --clearScreen false",
    "prepack": "tsc -b && vite build",
    "build": "tsc -b && vite build",
    "check": "tsc -b tsconfig.json",
    "test": "vitest",
    "lint": "eslint \"{src,test}/**/*.{ts,tsx}\"",
    "docgen": "docgen"
  },
  "dependencies": {
    "@formkit/auto-animate": "^0.8.2",
    "@effect-atom/atom": "latest",
    "@effect-atom/atom-react": "latest",
    "@effect/cluster": "^0.56.2",
    "@effect/platform": "latest",
    "@effect/platform-bun": "latest",
    "@effect/platform-node": "latest",
    "@fontsource-variable/geist": "^5.2.8",
    "@fontsource-variable/geist-mono": "^5.2.7",
    "@knpkv/codecommit-core": "workspace:^",
    "@tailwindcss/typography": "^0.5.19",
    "@tailwindcss/vite": "^4.1.18",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "effect": "latest",
    "lucide-react": "^0.563.0",
    "radix-ui": "^1.4.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-markdown": "^10.1.0",
    "react-router": "^7.0.0",
    "rehype-sanitize": "^6.0.0",
    "remark-gfm": "^4.0.1",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.18",
    "tw-animate-css": "^1.4.0"
  },
  "devDependencies": {
    "@effect/vitest": "latest",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.0.3",
    "@vitejs/plugin-react": "^4.2.1",
    "typescript": "^5.7.2",
    "vite": "^5.0.0",
    "vitest": "latest"
  }
}
